{
    "presets": [
      ["es2015", { "modules": false }],
      ["react"]
    ],
    
    "env": {
      "test": {
        "presets": ["es2015"]
      },
      "production": {
        "plugins": [
          [
            "transform-react-remove-prop-types",
            {
              "removeImport": true
            }
          ]
        ]
      }
    }
}
